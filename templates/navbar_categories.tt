[% IF common_categories %]
  <div id="navbar_categories">
    <div class="navbar_group_title">Categories:</div>
    <ul class="navbar_group">
      [% FOREACH cat = [ 'Cafes', 'Coffee Shops', 'Pubs', 'Restaurants',
                         'Supermarkets', 'Cinemas', 'Theatres' ] %]
