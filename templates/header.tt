<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">

<html>
<head>
  <title>[% node_name %] - [% site_name %]</title>
  [% IF contact_email %]
    <link rev="made" href="mailto:[% contact_email %]" />
    <link rel="meta" type="application/rdf+xml" title="RDF for this node" href="[% full_cgi_url %]?id=[% node_param %];format=rdf" />
  [% END %]
  [% IF description %]
    <meta name="description" content="[% description %]" />
  [% END %]
  [% IF keywords %]
    <meta name="keywords" content="[% keywords %]" />
  [% END %]
  [% IF stylesheet %]
    <link rel="stylesheet" href="[% stylesheet %]" type="text/css" />
  [% END %]
  [% IF latitude AND longitude %]
    <meta name="ICBM" content="[% latitude %], [% longitude %]" />
    <meta name="DC.title" content="[% site_name %]" />
  [% END %]
</head>

<body>
<div id="content">

[% INCLUDE navbar.tt %]
