[% INCLUDE header.tt %]
[% INCLUDE banner.tt %]
<div id="content">
[% INCLUDE navbar.tt %]
<h1 align=center>
  [% site_name %] - Backlinks to <span class="node_name">[% node_name %]</span>
</h1>

<p>Results found: [% num_results %]</p>

<ul>
  [% FOREACH hit = results %]
    <li><a href="[% cgi_url %]?[% hit.url %]">[% hit.title %]</a></li>
  [% END %]
</ul>

[% INCLUDE footer.tt %]
