[% INCLUDE header.tt %]
<div id="header">
<h1><a href="?Home" class="home_link">[% site_name %]</a></h1>
</div>
<div id="content">
<table border="0" cellpadding="0" cellspacing="0">
	<tr>
		<td><h2>Search Results for [% search_terms %]</h2>
		<p>
		[% num_results %] pages found:
		</p>
		<ul>
		[% FOREACH hit = results %]
			<li><a href="[% cgi_url %]?[% hit.url %]">[% hit.title %]</a></li>
		[% END %]
		</ul></td>
		<td valign="top">[% INCLUDE navbar.tt %]</td>
	</tr>
</table>

[% INCLUDE footer.tt %]
