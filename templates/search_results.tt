[% INCLUDE header.tt %]
<div id="content">
<h1 align=center>[% site_name %] - Search Results</h1>

<p>Searching on <b>[% search_terms %]</b>:</p>

<p>[% num_results %] pages found:</p>

<ul>
  [% FOREACH hit = results %]
    <li><a href="[% cgi_url %]?[% hit.url %]">[% hit.title %]</a></li>
  [% END %]
</ul>

[% INCLUDE footer.tt %]
